<%= form_for(@vendor) do |f| %>
	<% if @vendor.errors.any? %>
		<div id="error_explanation" >
			Error!
		</div>
	<% end %>
	
	<div class="field">
		<%= f.label :name %><br />
		<%= f.text_field :name %>
	</div>
	<div class="field">
		<%= f.label :category %><br />
		<%= f.text_field :category %>
	</div>
	<div class="field">
		<%= f.label :address1 %><br />
		<%= f.text_field :address1 %>
	</div>
	<div class="field">
		<%= f.label :address2 %><br />
		<%= f.text_field :address2 %>
	</div>
	<div class="field">
		<%= f.label :city %><br />
		<%= f.text_field :city %>
	</div>
	<div class="field">
		<%= f.label :state %><br />
		<%= f.text_field :state %>
	</div>
	<div class="field">
		<%= f.label :zip %><br />
		<%= f.text_field :zip %>
	</div>
	<div class="actions">
    	<%= f.submit %>
    </div>
<% end %>