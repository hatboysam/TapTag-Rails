<!DOCTYPE html>
<html>
<head>
  <title>TapT.ag</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="container">
		<% flash.each do |name, msg| %>
			<div id = <%= "flash_#{name}" %> >
				<%= msg %>
			</div>
		<% end %>
		<%= yield %>
	</div>
</body>
</html>
