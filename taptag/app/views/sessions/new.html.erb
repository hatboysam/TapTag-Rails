<h2>Sign In</h2>
<%= form_tag sessions_path do %>
	<div class="field">
		<%= label_tag(:email, "Email: ") %><br />
		<%= text_field_tag(:email) %>
	</div>
	<div class="field">
		<%= label_tag(:password, "Password: ") %><br />
		<%= password_field_tag(:password) %>
	</div>
	<div class="submit">
		<%= submit_tag("Log In") %>
	</div>
<% end %>